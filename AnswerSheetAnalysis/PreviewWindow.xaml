        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PreviewWindow" Width="570" Height="820">
    <Grid>
        <Border BorderThickness="2" BorderBrush="Black" Width="540" Height="780">
            <Grid>
                <InkCanvas x:Name="PreviewCanvas" Background="LightYellow" EditingMode="None"/>
                <Canvas x:Name="StepGraphCanvas" />
            </Grid>
        </Border>
    </Grid>
</Window>
